  "editor.tokenColorCustomizations": {
    "[Seti Black]":
      {
        "textMateRules": [
        {
          "scope": "variable.parameter",
          "settings": {
            "fontStyle": "",
            "foreground":"#e7983e" //"#d19438"
            }
        },
        {
          "scope": [
            "punctuation.separator.pointer-access.cpp"
          ],
          "settings": {
            "foreground": "#d7d7d7ff"
          }
        },
        {
          "scope": [
            "keyword.control.return"
          ],
          "settings": {
            "fontStyle": "bold",
            "foreground": "#fd2868"
          }
        },
        {
          "scope": "keyword.control",
          "settings": {
            "foreground": "#4b93d6"
          }
        },
        {
          "scope": [
            "keyword.control.directive",
            "keyword.other.typedef",
            "keyword.control.import",
            "entity.name.function.decorator.python"
          ],
          "settings": {
            "foreground": "#fd2868"//"#e6008e"
          }
        },
        {
          "scope": [
            "keyword.operator",
            "keyword.operator.logical",
            "keyword.operator.assignment"
          ],
          "settings": {
            "foreground": "#fd2868"
          }
        },
        {
          "scope": "storage.modifier.reference",
          "settings": {
            "foreground": "#b1b1b1"
          }
        },
        {
          "scope": [
            "entity.name.function",
            "entity.name.class",
            "entity.other.inherited-class",
            "support.function",
            "support.class"],
          "settings": {
            "fontStyle": "",
            "foreground": "#7dd443"
          }
        },
        {
          "scope": [
            "storage.type",
            "entity.name.type"
          ],
          "settings": {
            "fontStyle": "",
            "foreground": "#6CA9E1"//"#0fa6cc"
          }
        },
        {
          "scope": [
            "storage.modifier.specifier.const",
            "storage.type.struct",
            "storage.type.class"
            ],
          "settings": {
            "foreground": "#0de6b7"
          }
        },
        {
          "scope": [
            "string.quoted.double",
            "string.quoted.single"
          ],
          "settings": {
            "foreground": "#ecea70"
          }
        },
        {
          "scope":[
            "storage.type.class.doxygen"
          ],
          "settings": {
            "foreground": "#999797",
            "fontStyle": "bold"
          }
        },

        // Markdown Specific
        {
          "scope":[
            "markup.heading"
          ],
          "settings": {
            "foreground": "#6398c4",
            "fontStyle": "bold"
          }
        },
        {
          "scope":[
            "string.other.link.title.markdown"
          ],
          "settings": {
            "foreground": "#fdc959",
          }
        },
        {
          "scope": [
            "punctuation.definition.list.begin.markdown",
            "markup.bold.markdown"
          ],
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope":[
            "markup.italic.markdown"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": [
            "markup.quote.markdown"
          ],
          "settings": {
            "foreground": "#76eeb2",
          }
        }
      ]
      },

      "[Gatito Theme]":
      {
        "textMateRules": [
          {
            "scope": ["comment"],
            "settings": {
              "foreground": "#7F7F7F",
              "fontStyle": ""
            }
          },
          {
            "scope": ["variable.other.enummember"],
            "settings": {
              "foreground": "#e4767b",
              "fontStyle": ""
            }
          },
          {
            "scope": ["variable.language.this",
                      "variable.language.special.self"],
            "settings": {
              "foreground": "#e4767b",
              "fontStyle": ""
            }
          },
          {
            "scope": ["keyword.other.unit"],
            "settings": {
              "foreground": "#58865A",
              "fontStyle": ""
            }
          },
          {
            "scope": ["constant.numeric", "constant.language",
                      "constant.other"],
            "settings": {
              "foreground": "#97BC62FF",
              "fontStyle": "",
            }
          },
          {
            "scope": ["entity.name.class", "entity.name.type.class",
                      "support.type", "support.class"],
            "settings": {
              "foreground": "#e9bf6c",
              "fontStyle": ""
            }
          },
          {
            "scope": ["punctuation", "meta.function-call.arguments.python"],
            "settings": {
              "foreground": "#D4D4D4",
              "fontStyle": ""
            }
          }
        ]
      }
  },
