{
  // Common editor settings
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "window.menuBarVisibility": "hidden",
  "window.enableMenuBarMnemonics": false,
  "editor.suggestSelection": "first",
  "extensions.ignoreRecommendations": true,
  "window.titleSeparator": " ~ ",
  "window.title": "${dirty}${separator}${rootName}",
  "workbench.sideBar.location": "right",
  "workbench.editor.untitled.hint": "hidden",
  "editor.guides.bracketPairs": false,
  "editor.guides.highlightActiveIndentation": true,
  // "files.trimTrailingWhitespace": true,

  // terminal
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell",
      "args": ["-NoProfile"]
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Xonsh": {
      "path": [
        "xonsh"
      ]
    }
  },

  "terminal.integrated.defaultProfile.windows": "Xonsh",
  "terminal.integrated.automationProfile.windows": {
    "path": "${env:windir}\\System32\\cmd.exe",
  },

  "editor.bracketPairColorization.enabled": false,

  // language specific tool paths, settings
  // Python
  "python.pythonPath": "C:\\Anaconda3\\python.exe",
  "python.languageServer": "Jedi",
  "jupyter.sendSelectionToInteractiveWindow": false,
  "jupyter.askForKernelRestart": false,
  "jupyter.runStartupCommands": [
      "%config IPCompleter.use_jedi = False"
  ],
  "jupyter.interactiveWindowMode": "perFile",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },

  // C/C++
  "C_Cpp.enhancedColorization": "Enabled",
  "C_Cpp.errorSquiggles": "Disabled",
  "C_Cpp.autocomplete": "Disabled",
  "C_Cpp.intelliSenseEngine": "Disabled",
  "clangd.path": "/home/murali/.config/Code/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/14.0.3/clangd_14.0.3/bin/clangd",
  "clangd.checkUpdates": true,

  // Git
  "git.enabled": false,
  "git.autofetch": false,
  "git.path": null,
  "git.mergeEditor": true,

  // Debugging
  "debug.showBreakpointsInOverviewRuler": true,

  // Performance Improvements
  "telemetry.telemetryLevel": "off",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "emmet.excludeLanguages": [
    "markdown",
    "python",
    "c",
    "cpp"
  ],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/CMakeFiles": true
  },
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/node_modules/**": true,
    "**/CMakeFiles/**":true
  },
  "files.exclude": {
    "**/*.obj": true,
    "**/*.pdb": true,
    "**/*.ilk": true,
    "**/*.dll": true
  },
  
  "workbench.activityBar.visible": false,
  "diffEditor.renderSideBySide": false,
  "workbench.layoutControl.enabled": false,

  // Extensions ------------
  "workbench.colorTheme": "Gatito Theme",
  "redhat.telemetry.enabled": false,

